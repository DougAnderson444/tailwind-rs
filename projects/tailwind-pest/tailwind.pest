lists = {lines ~ EOI }
lines = _{ line ~ (next_line)* }
line = { "-" ~ item ~ (children)? }
next_line = _{ identation ~ line }

identation = ${ "\n" ~ PEEK_ALL }
identation_push = ${ identation ~ PUSH(space) }

space = _{ (" " | "\t")+ }
children = { identation_push ~ lines ~ DROP }

WHITESPACE = _{ " " }
ALPHA = _{ 'a'..'z' | 'A'..'Z' }
word = ${ ALPHA+ }
item = _{ word+ }